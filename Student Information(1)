#include <iostream>
#include <string> // Need to include <string> for string data type
using namespace std;

class Student {
public:
    string name;
    string branch;
    string subject;
    string year;
    float result;
};

int main() {
   Student s1;

     s1.name = "ABC";
    s1.branch = "E&TC";
    s1.subject = "C++";
    s1.result = 8.6;
   cout << s1.name << endl;
    cout << s1.branch << endl;
    cout << s1.subject << endl;
    cout << s1.year << endl; // Will output an empty line
    cout << s1.result << endl;

    return 0;
}
